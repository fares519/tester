        ["Bring Player"] = "bring",
        ["Drop Player"] = "drop",
        ["Loop Knock Player"] = "loopknock",
        ["Reset Stand"] = "reset",
        ["Bend Player"] = "bend",
        ["Gun Bring Player"] = "gbring",
        ["Loop Gun Knock Player"] = "loopgknock",
        ["Loop Gun Kill Player"] = "loopgkill",
        ["Gun Kill Player"] = "gkill",
